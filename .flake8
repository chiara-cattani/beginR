# Flake8 configuration
[flake8]
max-line-length = 127
exclude = 
    .git,
    __pycache__,
    venv,
    env,
    .venv,
    .dev,
    migrations,
    .tox,
    .coverage,
    htmlcov,
    node_modules
ignore = 
    E203,
    E501,
    W503
per-file-ignores =
    __init__.py:F401
max-complexity = 10